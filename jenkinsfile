pipeline {
    agent any
    stages{
        stage('stage1'){
            steps{
                //git 'https://github.com/iamsnjy/MyJenkinsPipeline1_demo.git'
                echo "stage1 of pipeline"
            }
        }
        
        
        stage('Hello') {
            steps {
                echo 'Hello World'
                sh 'pwd'
                sh 'ls -l'
                sh 'python3 --version'
            }
        }
    }
}

