pipeline {
    agent any
    stages{
        stage('stage 1'){
            steps{
                //git 'https://github.com/iamsnjy/MyJenkinsPipeline1_demo.git'
                echo "stage1 of pipeline"
                echo 'agent any means, using the local host server env for running the pipeline script and its actions.'
                echo 'agent can be set to i) docker image'
            }
        }
        
        
        stage('stage 2') {
            steps {
                echo 'Hello World'
                sh 'pwd'
                sh 'ls -l'
                sh 'python3 --version'
                echo 'Running a python file...'
                sh 'python3 coding/1_frequency.py'

            }
        }
    }
}

